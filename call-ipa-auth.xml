<edgeservices:processing-agent>
    <status>on</status>
    <log-r-line>on</log-r-line>
    <url>%(PMUSER_IPA_PATH)</url>
    <host>%(PMUSER_IPA_HOST)</host>
    <post-body></post-body>
    <skip-on-previous-failure>off</skip-on-previous-failure>
    <authenticate>on</authenticate>
    <cookies>
        <client-to-agent>*</client-to-agent>
        <agent-to-origin>*</agent-to-origin>
        <agent-to-client>*</agent-to-client>
    </cookies>
</edgeservices:processing-agent>